@model List<CourseManagementMVC.Models.Course>

    <table>
        <thead>
            <tr>
                <th>Course Name</th>
                <th>Description</th>
                <th>Course Code</th>
                <th>Is Active</th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.CourseName</td>
                <td>@item.CourseDescription</td>
                <td>@item.CourseCode</td>
                <td>@item.IsActive</td>
            </tr>


            }
        </tbody>
    </table>

    @{
    }
